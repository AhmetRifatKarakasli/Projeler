@model IEnumerable<My_Project.Models.Category>
<ul class="dropdown-menu">
    @foreach (var cate in Model)
    {
        <li>
            <a class="dropdown-item" href="@Url.Action("Pages", "Product", new { id = cate.cate_id })">
                @cate.cate_name
            </a>
        </li>
    }
</ul>