
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h2>
        Welcome to my Website
    </h2>
</div>

<div>
    <div id="carouselExample" class="carousel slide">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="~/Products_images/fahri.png" class="d-block w-100" alt="..." height="1000" width="1000">
            </div>
            <div class="carousel-item">
                <img src="~/Products_images/memati.png" class="d-block w-100" alt="..." height="1000" width="1000">
            </div>
            <div class="carousel-item">
                <img src="~/Products_images/travis.png" class="d-block w-100" alt="..." height="1000" width="1000">
            </div>
        </div>
